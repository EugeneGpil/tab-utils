{
  "scripts": {
    "dev": "yarn goldpage:dev",
    "dev:link": "ln -s `pwd`/static/* `pwd`/tab-utils/static/* .build/browser/",
    "prod:build": "yarn prod:build:clean && yarn goldpage:build && yarn prod:build:static && yarn prod:build:link && yarn prod:build:deployId",
    "prod:build:link": "(cd .build/browser/ && mv tab-utils/ tab-utils-moved/ && ln -s tab-utils-moved/ tab-utils)",
    "prod:build:clean": "rm -rf .build/",
    "prod:build:static": "mkdir -p .build/browser/ && cp -r static/* tab-utils/static/* .build/browser/",
    "prod:build:deployId": "(cd .build/browser/ && grep __DEPLOY -rl | xargs sed -i \"s/__DEPLOY/$(cat /dev/urandom | tr -dc 'a-zA-Z0-9' | fold -w 8 | head -n 1)/g\")",
    "prod:serve": "serve .build/browser/ --listen 3000",
    "prod:deploy": "push-dir --dir=.build/browser/ --branch=gh-pages --cleanup",
    "goldpage:dev": "goldpage dev",
    "goldpage:build": "goldpage build"
  },
  "dependencies": {
    "@brillout/assert": "^0.1.7",
    "@brillout/json-s": "^0.1.0",
    "@brillout/website-legals": "^0.0.4",
    "@goldpage/react": "^0.5.6",
    "@goldpage/serverless": "^0.5.6",
    "@goldpage/typescript": "^0.5.6",
    "@risingstack/react-easy-state": "^6.2.3",
    "@types/react": "^16.9.34",
    "bowser": "^2.9.0",
    "core-js": "3",
    "flatpickr": "^4.6.3",
    "goldpage": "^0.5.6",
    "iframe-resizer": "^4.2.10",
    "js-cookie": "^2.2.1",
    "lodash.throttle": "^4.1.1",
    "luxon": "^1.24.1",
    "nanoid": "^3.1.3",
    "push-dir": "^0.4.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "serve": "^11.3.0",
    "stacktrace-js": "^2.0.2",
    "toastify-js": "^1.7.0",
    "typescript": "^3.8.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3"
  }
}
