{
  "scripts": {
    "// Dev": "",
    "dev": "yarn dev:goldpage",
    "dev:link": "ln -s `pwd`/static/* `pwd`/tab-utils/static/* .build/browser/",
    "dev:goldpage": "goldpage dev",
    "// [Prod] Build": "",
    "build": "yarn clean:all && yarn install && yarn build:goldpage && yarn build:static && yarn build:link && yarn build:deploy-id",
    "build:goldpage": "goldpage build",
    "build:static": "mkdir -p .build/browser/ && cp -r static/* .build/browser/",
    "build:link": "(cd .build/browser/ && mv tab-utils/ tab-utils-moved/ && ln -s tab-utils-moved/ tab-utils)",
    "build:deploy-id": "DEPLOY_ID=$(cat /dev/urandom | tr -dc 'a-zA-Z0-9' | fold -w 8 | head -n 1) && (cd .build/browser/ && grep __DEPLOY -rl | xargs sed -i \"s/__DEPLOY/$DEPLOY_ID/g\") && echo \"DEPLOY_ID: $DEPLOY_ID\"",
    "// [Prod] Serve": "",
    "serve": "serve .build/browser/ --listen 3000",
    "// [Prod] Deploy": "",
    "deploy": "yarn build && yarn deploy:github",
    "deploy:github": "push-dir --dir=.build/browser/ --branch=gh-pages --cleanup",
    "// [Prod] Clean": "",
    "clean": "yarn clean:all",
    "clean:all": "git clean -Xdf",
    "clean:build": "rm -rf .build/"
  },
  "dependencies": {
    "@brillout/assert": "^0.1.7",
    "@brillout/json-s": "^0.1.0",
    "@brillout/website-legals": "^0.0.6",
    "@goldpage/react": "^0.5.6",
    "@goldpage/serverless": "^0.5.6",
    "@goldpage/typescript": "^0.5.6",
    "@risingstack/react-easy-state": "^6.2.3",
    "@types/react": "^16.9.34",
    "bowser": "^2.9.0",
    "core-js": "3",
    "flatpickr": "^4.6.3",
    "goldpage": "^0.5.6",
    "iframe-resizer": "^4.2.10",
    "js-cookie": "^2.2.1",
    "lodash.throttle": "^4.1.1",
    "luxon": "^1.24.1",
    "nanoid": "^3.1.3",
    "push-dir": "^0.4.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "serve": "^11.3.0",
    "stacktrace-js": "^2.0.2",
    "toastify-js": "^1.7.0",
    "typescript": "^3.8.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3"
  }
}
