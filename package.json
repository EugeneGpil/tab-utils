{
  "name": "tab-utils",
  "version": "0.0.5",
  "scripts": {
    "build": "rm -rf dist/ && parcel build *.js",
    "prepublishOnly": "yarn build && yarn moveProdFiles",
    "moveProdFiles": "cp ./dist/*.js ./",
    "postpublish": "git checkout *.js"
  },
  "files": [
    "*.js",
    "*.js.map"
  ],
  "devDependencies": {
    "parcel": "^1.12.4"
  },
  "dependencies": {
    "core-js": "3"
  }
}
